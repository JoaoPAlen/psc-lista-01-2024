programa {
  funcao inicio() 
  {
      cadeia nome
      inteiro idade
      real altura

      escreva("Olá, ", nome)
      leia(nome)
      escreva("Qual sua idade? ", idade)
      leia(idade)
      escreva("Qual sua altura? ", altura)
      leia(altura)
      escreva(nome, " tem ", idade, " anos e ", altura, " de altura.")

  }
}
